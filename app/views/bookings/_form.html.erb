<div id='booking_date_picker' data-unavailable="<%= @car.unavailable_dates.to_json %>">
  <% if current_user.id != @car.user_ids  %>
    <%= simple_form_for [@car, @booking] do |f| %>
      <%= f.input :start_date, as: :string, input_html: { id: "booking_start_date"} %>
      <%= f.input :end_date, as: :string, input_html: { id: "booking_end_date"} %>
      <%= f.submit "Confirm", class: "btn btn-primary" %>
      <%= f.error_notification %>
    <% end %>
  <% end %>
</div>
